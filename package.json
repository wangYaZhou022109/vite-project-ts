{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:qa": "vue-tsc --noEmit && vite build --mode qa",
    "build:sim": "vue-tsc --noEmit && vite build --mode sim",
    "build": "vue-tsc --noEmit && vite build --mode online",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "lint-staged": "^10.5.4",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.0",
    "vue-tsc": "^0.0.8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
